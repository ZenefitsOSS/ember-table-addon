<div class="et-cell-container">
  <span {{bind-attr class=":et-toggle-span :et-toggle
      isCollapsed:ember-table-expand:et-collapse"}}
      {{action 'toggleTableCollapse'}}>
    <i class="icon-caret-down et-toggle-icon"></i>
  </span>
  <div class="et-header-content-container">
    <span class="et-content">
      {{view.column.headerCellName}}
    </span>
  </div>
</div>
